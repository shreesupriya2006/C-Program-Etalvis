#include <stdio.h>
void disp_single_digit_square(int);
int main()
{
    int n;
    scanf("%d",&n);
    disp_single_digit_square(n);
}
void disp_single_digit_square(int n)
{
    int count=0;
    loop:if(n>0)
    {
        int n1=n%10;
        int i=n1;
        loop1:if(i>0)
        {
            if(i*i==n1)
            count++;
            i--;
        goto loop1;}
        n=n/10;
        goto loop;
        
    }
    printf("%d",count);
}
