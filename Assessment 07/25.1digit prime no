#include <stdio.h>
void disp_single_digit_prime(int);
int main()
{
    int n;
    scanf("%d",&n);
    disp_single_digit_prime(n);
}
void disp_single_digit_prime(int n)
{
    int count=0;
    loop:if(n>0)
    {
        int count1=0;
        int n1=n%10;
        int i=n1;
        loop1:if(i>0){
        if(n1%i==0){
            count1++;
        }
            i--;
            goto loop1;
        }
        n=n/10;
        if(count1==2)
        count++;
        goto loop;
    }
    printf("%d",count);
}
