#include <stdio.h>
void disp_biggest_4digit_div7_9(int);
int main()
{
  int n=1000;
  disp_biggest_4digit_div7_9(n);
}
void disp_biggest_4digit_div7_9(int n)
{
    int i=n;
    loop:if(i<10000)
    {
        if((i%7==0)&&(i%9==0))
        n=i;
        i++;
        goto loop;
    }
    printf("%d",n);
}
