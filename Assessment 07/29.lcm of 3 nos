#include <stdio.h>
void disp_LCM3(int,int,int);
int main()
{
    int n1,n2,n3;
    scanf("%d %d %d",&n1,&n2,&n3);
    disp_LCM3(n1,n2,n3);
    return 0;
}
void disp_LCM3(int n1,int n2,int n3)
{
    int lcm=0;
    int max=n1;
    if(n2>max)
    max=n2;
    if(n3>max)
    max=n3;
    while(1){
    if((max%n1==0)&&(max%n2==0)&&(max%n3==0))
    {
        lcm=max;
        break;

    }
   max++;
   }
   printf("%d",lcm);

}
