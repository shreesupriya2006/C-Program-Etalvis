#include <stdio.h>
void disp_total_odd_digit(int);
int main()
{
    int n;
    scanf("%d",&n);
    disp_total_odd_digit(n);
}
void disp_total_odd_digit(int n)
{ 
    int count=0;
    loop:if(n>0)
    {
        int n1=n%10;
        if(n1%2==1)
        {
            count++;
        }
        n=n/10;
        goto loop;
    }
    printf("%d",count);
}
