#include <stdio.h>
void disp_1digit_prime(int);
int main()
{
    int n;
    scanf("%d",&n);
    disp_1digit_prime(n);
}
void disp_1digit_prime(int n)
{
    int count=0;
    loop:if(n>=10)
    {
        int count1=0;
        int n1=n%10;
        int i=n1;
        loop1:if(i>0){
        if(n1%i==0)
        {
            count1++;
        }
        i--;
            goto loop1;
        }
        if(count1==2)
        count++;
        n=n/10;
        goto loop;
        
    }
    printf("%d",count);
}
