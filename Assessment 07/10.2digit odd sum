#include <stdio.h>
void disp_2digit_odd_sum_tens7(int);
int main()
{
    int a=10;
    disp_2digit_odd_sum_tens7(a);
}
void disp_2digit_odd_sum_tens7(int a)
{
    int sum=0;
    for(int i=a;i<100;i++)
    {
        if(i%2==1)
        {
            int n1=i/10;
            if(n1==7)
            {
                sum=sum+i;
                
            }
        }
    }
    printf("%d",sum);
}
