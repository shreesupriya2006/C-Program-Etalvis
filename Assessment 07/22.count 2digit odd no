#include <stdio.h>
void disp_total_2digit_odd(int);
int main() 
{
    int n;
    scanf("%d",&n);
    disp_total_2digit_odd(n);
}
void disp_total_2digit_odd(int n)
{
    int count=0;
    loop:if(n>=10){
    int n1=n%10;
    int n2=(n/10)%10;
    int sum=n2*10+n1;
    if(sum%2==1)
    count++;
    n=n/10;
    goto loop;}
    printf("%d",count);
}
