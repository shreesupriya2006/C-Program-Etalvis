#include <stdio.h>
void disp_2digit_even_sum6(int);
int main()
{
    int a=10;
    disp_2digit_even_sum6(a);
}
void disp_2digit_even_sum6(int a)
{
    int sum=0;
    for(int i=a;i<100;i++)
    {
        if(i%2==0)
        {
            int n1=i/10;
            int n2=i%10;
            sum=n1+n2;
            if(sum==6)
            {
                printf("%d\n",i);
            }
        }
    }
}
