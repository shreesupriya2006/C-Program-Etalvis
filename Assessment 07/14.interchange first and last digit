#include <stdio.h>
void disp_interchange_first_last_digit(int);
int main()
{
    int n;
    scanf("%d",&n);
    disp_interchange_first_last_digit(n);
}
void disp_interchange_first_last_digit(int n)
{
    int power=1;
    int n2=n%10;
    int temp=n;
    while(temp>=10)
    {
        temp=temp/10;
        power=power*10;
        
    }
    int n1=temp;
    int middle=(n%power)/10;
    int result=n2*power+middle*10+n1;
    printf("%d",result);
}
