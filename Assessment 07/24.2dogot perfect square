#include <stdio.h>
void disp_two_digit_square(int);
int main()
{
    int n;
    scanf("%d",&n);
    disp_two_digit_square(n);
}
void disp_two_digit_square(int n)
{
    int count=0;
    loop:if(n>=10)
    {
        int n1=n%10;
        int n2=(n/10)%10;
        int sum=n2*10+n1;
        int i=sum;
        loop1:if(i>0)
        {
            if(i*i==sum)
            count++;
            i--;
        goto loop1;}
        n=n/10;
        goto loop;
        
    }
    printf("%d",count);
}
