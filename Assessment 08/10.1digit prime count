#include <stdio.h>
int disp_1digit_prime(int);
int main()
{
    int n=2,count;
    count=disp_1digit_prime(n);
    printf("%d",count);
}
int disp_1digit_prime(int a)
{
    int count=0;
    loop:if(a<10)
    {
    int prime=1;
    for(int i=2;i<a;i++){
        if(a%i==0)
        prime=0;}
    if(prime)
    count++;
    a++;
    goto loop;
    }
    return count;
}
