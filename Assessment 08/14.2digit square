#include <stdio.h>
int disp_two_digit_square(int);
int main()
{
    int n,count;
    scanf("%d",&n);
    count=disp_two_digit_square(n);
    printf("%d",count);

}
int disp_two_digit_square(int a)
{
    int count=0,i;
    int temp=a;
    loop:if(temp>=10)
    {
        int n1=temp%10;
        int n2=(temp/10)%10;
        int sum=n2*10+n1;
        for(i=1;i<=sum;i++)
        {
            if(i*i==sum)
            count++;
        }
        temp=temp/10;
        goto loop;
    }
    return count;
}
