#include <stdio.h>
int disp_single_digit_square(int);
int main()
{
    int n,count;
    scanf("%d",&n);
    count=disp_single_digit_square(n);
    printf("%d",count);

}
int disp_single_digit_square(int a)
{
    int count=0,i;
    int temp=a;
    loop:if(temp>0)
    {
        int n1=temp%10;
        for(i=1;i<=n1;i++)
        {
            if(i*i==n1)
            count++;
        }
        temp=temp/10;
        goto loop;
    }
    return count;

}
