#include <stdio.h>
int disp_LCM2(int,int,int);
int main()
{
    int n1,n2,n3,lcm;
    scanf("%d %d %d",&n1,&n2,&n3);
    lcm=disp_LCM2(n1,n2,n3);
    printf("%d",lcm);
}
int disp_LCM2(int a,int b,int c)
{
    int max=a,lcm=0;
    if(b>max)
        max=b;
    if(c>max)
        max=c;
    loop:{
    if((max%a==0)&&(max%b==0)&&(max%c==0)){
        lcm=max;
        goto end;}
        max++;
        goto loop;
    }
    end:
    return lcm;
}
