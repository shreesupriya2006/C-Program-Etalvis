#include <stdio.h>
int disp_hcf2(int,int);
int main()
{
    int n1,n2,hcf;
    scanf("%d %d",&n1,&n2);
    hcf=disp_hcf2(n1,n2);
    printf("%d",hcf);
}
int disp_hcf2(int a,int b)
{
    int min,hcf=1;
    if(a<b)
        min=a;
    else
        min=b;
    int i=1;
    while(i<=min){

    if((a%i==0)&&(b%i==0))
        hcf=i;
    i++;
       }
    return hcf;

}
