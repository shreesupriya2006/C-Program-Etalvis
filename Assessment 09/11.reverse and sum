#include <stdio.h>
int main()
{
    int a[5];
    int b[5];
    int i,j=0,sum=0;
    for(i=0;i<5;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<5;i++)
    {
        int rev=0;
        int temp=a[i];
        while(temp>0)
        {
            int n1=temp%10;
            rev=rev*10+n1;
            temp=temp/10;
        }
        b[j]=rev;
        j++;
    }
    for(i=0;i<5;i++)
    {
        sum=sum+b[i];
    }
    printf("%d",sum);
}
