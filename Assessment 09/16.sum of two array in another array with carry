#include <stdio.h>
int main()
{
    int a[50],b[50],sum[51];
    int n,carry=0,i;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        scanf("%d",&b[i]);
    }
    for(i=n-1;i>=0;i--)
    {
        sum[i+1]=a[i]+b[i]+carry;
        carry=sum[i+1]/10;
        sum[i+1]=sum[i+1]%10;
    }
    sum[0]=carry;
    if(sum[0]!=0)
        printf("%d",sum[0]);
    for(i=1;i<=n;i++)
    {
        printf("%d",sum[i]);
    }
}
