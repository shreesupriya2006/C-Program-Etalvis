#include <stdio.h>
int main()
{
   int n,count=0;
   scanf("%d",&n);
   int temp=n;
   loop:if(n>10)
   {
       int n2=n%10;
       temp=n/10;
       int n1=temp%10;
       int digit=n1*10+n2;
       if(digit%2==1)
       {
           count++;
       }
       n=n/10;
       goto loop;
   }
   printf("%d",count);
   
}
