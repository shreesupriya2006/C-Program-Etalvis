#include <stdio.h>
int main()
{
    int n,count=0;
    scanf("%d",&n);
    while(n>0)
    {
       int n1=n%10;
       if(n1==2||n1==3||n1==5||n1==7)
       {
           count++;
       }
       n=n/10;
        
    }
    printf("%d",count);
}
