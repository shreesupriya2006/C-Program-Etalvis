#include <stdio.h>
int main()
{
    int n,n1,n2,sum=0,count=0;
    scanf("%d",&n);
    while(n>=10)
    {
        n2=n%10;
        n1=n/10;
        n1=n1%10;
        sum=n1*10+n2;
        int i=9;
        while(i>0)
        {
            if(i*i==sum){
            count++;
            break;}
            i--;
        }
        n=n/10;
    }
    printf("%d",count);
}
