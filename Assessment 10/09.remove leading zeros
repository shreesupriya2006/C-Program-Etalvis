#include <stdio.h>
#include <string.h>
int main()
{
    int i=0,cnt=0,len;
    char a[50];
    scanf("%s",a);
    len=strlen(a);
    while(a[i]!='\0')
    {
        if(a[i]=='0')
        cnt++;
        else
        break;
        i++;
    }
    if(cnt==len){
    printf("0");
    return 0;}
    for(i=cnt;i<len;i++)
    printf("%c",a[i]);
}
