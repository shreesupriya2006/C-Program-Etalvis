/*Pass an integer array to a function and increment each array element inside the function.*/
#include <stdio.h>
void increment(int,int);
int main()
{
    int n,i;
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    increment(a,n);
    for(i=0;i<n;i++)
    {
        printf("%d",a[i]);
    }
}
void increment(int *p,int n)
{
    int i;
    for(i=0;i<n;i++)
    {
        *p=*p+1;
        p++;
    }
}
