#include <stdio.h>
int main()
{
    int n,n1,power=1;
    scanf("%d",&n);
    int n2=n%10;
    int temp=n;
    loop:if(temp>=10)
    {
        power=power*10;
        temp=temp/10;
        goto loop;
    }
    n1=temp;
    if(n1%2==1)
    {
        int middle=(n%power)/10;
        int result=(n1-1)*power+middle*10+n2;
        printf("%d",result);
    }
    else
    printf("%d",n);
    
}
