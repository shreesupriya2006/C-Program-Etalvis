#include <stdio.h>
int main()
{
    int n1,n2,n3,max,lcm;
    scanf("%d %d %d",&n1,&n2,&n3);
    max=n1;
    if(max<=n2)
    max=n2;
    if(max<=n3)
    max=n3;
    loop:{
    if((max%n1==0)&&(max%n2==0)&&(max%n3==0))
    {
        lcm=max;
        goto end;
    }
    max++;
    goto loop;
    }
    end:
    printf("%d",lcm);
}
