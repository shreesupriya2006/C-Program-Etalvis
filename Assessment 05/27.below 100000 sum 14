#include <stdio.h>
int main()
{
  int n=1,count=0;
  while(n<100000)
  {
      int temp=n,sum=0;
      loop:if(temp>0)
      {
          int n1=temp%10;
          sum=sum+n1;
          temp=temp/10;
          goto loop;
      }
      if(sum==14)
      count++;
      n++;
  }
  printf("%d",count);
}
