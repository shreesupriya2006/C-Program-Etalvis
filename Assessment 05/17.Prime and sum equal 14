#include <stdio.h>
int main()
{
    int n,prime=1,n1,n2,sum=0;
    scanf("%d",&n);
    int i=2;
    while(i<n)
    {
        n1=n/10;
        n2=n%10;
        sum=n1+n2;
        if(n%i==0)
        {
            prime=0;
            break;
        }
        i++;
    }
    if(prime)
    {
        if(sum==14)
        printf("Prime & Sum is 14");
        else
        printf("Prime but sum is not 14");
    }
    else
    {
        if(sum==14)
        printf("Not Prime but Sum is 14");
        else
        printf("Not prime and sum of digit is not 14");
    }
}
