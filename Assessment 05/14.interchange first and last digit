#include <stdio.h>
int main()
{
    int n,n1,n2,power=1;
    scanf("%d",&n);
    n2=n%10;
    int temp=n;
    loop:if(temp>=10)
    {
        power=power*10;
        temp=temp/10;
        goto loop;
    }
    n1=temp;
    int middle=n%power;
    middle=middle/10;
    int result=n2*power+middle*10+n1;
    printf("%d",result);
}
